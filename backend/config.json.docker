{
  "langfuse": {
    "secret_key": "your_secret_key_here",
    "public_key": "your_secret_key_here",
    "host": "http://langfuse:3000",
    "debug": false,
    "tracing_enabled": true,
    "sample_rate": 1.0,
    "environment": "production",
    "release": "v1.0.0",
    "session_id": "",
    "user_id": ""
  },
  "llm": {
    "default_llm": "ollama-gemma3:4b",
    "providers": [
      {
        "name": "deepseek",
        "type": "deepseek",
        "api_key": "your_secret_key_here",
        "model_name": "deepseek-chat",
        "temperature": 0.1,
        "max_tokens": 2000,
        "enabled": true,
        "base_url": "https://api.deepseek.com",
        "model_info": {
          "family": "deepseek",
          "vision": false,
          "function_calling": true,
          "json_output": true,
          "structured_output": true
        }
      },
      {
        "name": "ollama-gpt-oss:20b",
        "type": "ollama",
        "api_key": "ollama",
        "model_name": "gpt-oss:20b",
        "temperature": 0.0,
        "max_tokens": 1500,
        "enabled": true,
        "base_url": "http://ollama:11434",
        "model_info": {
          "family": "ollama",
          "vision": false,
          "function_calling": true,
          "json_output": true,
          "structured_output": true
        }
      },
      {
        "name": "ollama-gemma3:4b",
        "type": "ollama",
        "api_key": "ollama",
        "model_name": "gemma3:4b",
        "temperature": 0.0,
        "max_tokens": 1500,
        "enabled": true,
        "base_url": "http://ollama:11434",
        "model_info": {
          "family": "ollama",
          "vision": false,
          "function_calling": true,
          "json_output": true,
          "structured_output": true
        }
      }
    ]
  },
  "neo4j": {
    "uri": "bolt://neo4j:7687",
    "username": "neo4j",
    "password": "password"
  },
  "agent": {
    "max_retries": 3,
    "checkpointer_type": "memory",
    "tracer_type": "console"
  },
  "server": {
    "host": "0.0.0.0",
    "port": 8000,
    "reload": false,
    "workers": 1,
    "access_log": true,
    "log_level": "info",
    "root_path": "",
    "openapi_url": "/openapi.json",
    "docs_url": "/docs",
    "redoc_url": "/redoc"
  },
  "logging": {
    "log_level": "INFO",
    "console_logging": true,
    "max_bytes": 10485760,
    "backup_count": 5,
    "log_file": "puntini.log",
    "logs_path": "logs"
  }
}
